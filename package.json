{
  "name": "@cognite/sdk-monorepo",
  "description": "Javascript client library for Cognite SDK",
  "contributors": [
    "Fredrik Anfinsen <fredrik.anfinsen@cognite.com>",
    "Ivan Polomanyi <ivan.polomanyi@cognite.com>",
    "Håvard Krogstie <havard.krogstie@cognite.com>",
    "Martin Røed <martin.roed@cognite.com>"
  ],
  "license": "Apache-2.0",
  "repository": "cognitedata/cognite-sdk-js",
  "homepage": "https://github.com/cognitedata/cognite-sdk-js#readme",
  "private": true,
  "keywords": [
    "cognite",
    "sdk",
    "typescript"
  ],
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "clean": "lerna run clean",
    "prepublishOnly": "yarn build --stream",
    "test": "lerna run test --stream",
    "lint": "lerna run lint --stream",
    "lint:fix": "lerna run lint:fix --stream",
    "install:all": "yarn",
    "build": "lerna run build --stream",
    "watch": "lerna run watch --parallel",
    "docs": "lerna run docs",
    "validateDocLinks": "find . -name \\*.md -not -path '*/node_modules/*' -exec yarn markdown-link-check {} \\;",
    "alphabetizeTypes": "node ./scripts/alphabetizeTypes.js -i src/types/types.ts -h \"// Copyright 2019 Cognite AS\"",
    "extract-snippets": "rm -rf codeSnippets/ && yarn docs --json codeSnippets/docs.json && node scripts/extractCodeSnippets.js",
    "test-snippets": "yarn link @cognite/sdk && yarn extract-snippets && yarn tsc -p codeSnippets/tsconfig.build.json && rm -rf codeSnippets/",
    "install-samples": "yarn workspace sdk-samples install:samples",
    "test-samples": "yarn workspace sdk-samples test:samples"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.1.0",
    "@commitlint/config-conventional": "^8.1.0",
    "@commitlint/travis-cli": "^8.2.0",
    "@types/es6-promise": "^3.3.0",
    "@types/jest": "^23.3.7",
    "@types/nock": "^10.0.3",
    "@types/node": "^10.12.0",
    "@types/query-string": "^6.1.0",
    "es-check": "^5.0.0",
    "husky": "^3.0.2",
    "jest": "^23.6.0",
    "lerna": "^3.22.1",
    "ls": "^0.2.1",
    "markdown-link-check": "^3.7.2",
    "nock": "^10.0.6",
    "parse-comments": "^1.0.0",
    "prettier": "^1.10.2",
    "rollup": "^0.67.0",
    "rollup-plugin-alias": "^2.2.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-typescript2": "^0.17.2",
    "swagger-parser": "^6.0.5",
    "ts-jest": "^23.10.4",
    "tslint": "^5.13.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.0.1",
    "tslint-sonarts": "^1.9.0",
    "typedoc": "^0.13.0",
    "typedoc-plugin-markdown": "^1.1.19",
    "typescript": "^3.3.3333",
    "typescript-parser": "^2.6.1"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
