{
  "name": "@cognite/sdk",
  "version": "2.0.0",
  "description": "Javascript client library for Cognite SDK",
  "contributors": [
    "Fredrik Anfinsen <fredrik.anfinsen@cognite.com>",
    "Ivan Polomanyi <ivan.polomanyi@cognite.com>",
    "Martin RÃ¸ed <martin.roed@cognite.com>"
  ],
  "license": "Apache-2.0",
  "repository": "cognitedata/cognitesdk-js",
  "homepage": "https://github.com/cognitedata/cognitesdk-js#readme",
  "main": "dist/index.js",
  "types": "dist/src/index.d.ts",
  "files": [
    "dist/"
  ],
  "keywords": [
    "cognite",
    "sdk",
    "typescript"
  ],
  "devDependencies": {
    "@types/es6-promise": "^3.3.0",
    "@types/jest": "^23.3.7",
    "@types/node": "^10.12.0",
    "@types/query-string": "^6.1.0",
    "axios-debug-log": "^0.4.0",
    "axios-mock-adapter": "1.15.0",
    "dtsgenerator": "^2.0.8",
    "es-check": "^5.0.0",
    "jest": "^24.8.0",
    "ls": "^0.2.1",
    "markdown-link-check": "^3.7.2",
    "mustache": "^3.0.1",
    "parse-comments": "^1.0.0",
    "prettier": "^1.10.2",
    "rollup": "^0.67.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-typescript2": "^0.17.2",
    "sleep-promise": "^8.0.1",
    "swagger-parser": "^8.0.0",
    "ts-jest": "^24.0.2",
    "tslint": "^5.13.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.0.1",
    "tslint-sonarts": "^1.9.0",
    "typedoc": "^0.14.2",
    "typedoc-plugin-markdown": "^2.0.8",
    "typescript": "^3.3.3333",
    "typescript-parser": "^2.6.1"
  },
  "scripts": {
    "clean": "rm -rf dist",
    "prepublishOnly": "yarn build",
    "test": "jest --config=jest.config.js",
    "test-samples": "cd samples && yarn link-sdk && yarn install-dependencies && yarn test",
    "validate-samples-sdk-version": "node ./scripts/validateSDKVersion.js",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "lint:fix": "prettier \"src/**/*.ts\" --write && tslint -c tslint.json -p tsconfig.json --fix",
    "build": "yarn clean && rollup -c && yarn esCheck",
    "watch": "rollup -cw",
    "docs": "typedoc --options typedoc.js --tsconfig tsconfig.json",
    "prepush": "yarn lint && yarn test",
    "esCheck": "es-check es5 './dist/index.js'",
    "validateDocLinks": "find . -name \\*.md -not -path '*/node_modules/*' -exec yarn markdown-link-check {} \\;",
    "alphabetizeTypes": "node ./scripts/alphabetizeTypes.js -i src/types/types.ts -h \"// Copyright 2019 Cognite AS\"",
    "extractSnippetsToJSON": "yarn docs --json docs.json && node scripts/postProcessDocs.js && rm docs.json"
  },
  "dependencies": {
    "axios": "0.18.1",
    "lodash": "^4.17.11",
    "query-string": "^5.1.1",
    "url": "^0.11.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
