{
  "private": false,
  "name": "@haved/cogsdk-core",
  "license": "Apache-2.0",
  "repository": "haved/cognite-sdk-js",
  "homepage": "https://github.com/haved/cognite-sdk-js#readme",
  "main": "dist/index.js",
  "types": "dist/index.d.js",
  "version": "0.2.5",
  "scripts": {
    "clean": "rm -rf dist",
    "prepublishOnly": "yarn build",
    "test": "jest --config=../../jest.config.js --testPathPattern=/core/",
    "lint": "tslint -c ../../tslint.json -p tsconfig.build.json",
    "lint:fix": "prettier \"src/**/*.ts\" --write && tslint -c ../../tslint.json -p tsconfig.build.json --fix",
    "build": "yarn clean && yarn rollup -c && yarn esCheck",
    "watch": "rollup -cw",
    "esCheck": "es-check es5 './dist/index.js'"
  },
  "dependencies": {
    "cross-fetch": "^3.0.4",
    "lodash": "^4.17.11",
    "query-string": "^5.1.1",
    "url": "^0.11.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
