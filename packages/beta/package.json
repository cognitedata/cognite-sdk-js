{
  "private": false,
  "name": "@haved/cogsdk-beta",
  "main": "dist/index.js",
  "types": "dist/src/index.d.js",
  "version": "2.32.1",
  "scripts": {
    "clean": "rm -rf dist/ docs/ codeSnippets/",
    "prepublishOnly": "yarn build",
    "test": "jest --config=../../jest.config.js --testPathPattern=/beta/",
    "lint": "tslint -c ../../tslint.json -p tsconfig.build.json",
    "lint:fix": "prettier \"src/**/*.ts\" --write && tslint -c ../../tslint.json -p tsconfig.build.json --fix",
    "build": "yarn clean && rollup -c && yarn esCheck",
    "watch": "rollup -cw",
    "docs": "typedoc --options typedoc.js --tsconfig tsconfig.json src/index.ts",
    "esCheck": "es-check es5 './dist/index.js'",
    "extract-snippets": "rm -rf codeSnippets/ && yarn docs --json codeSnippets/docs.json && node ../../scripts/extractCodeSnippets.js",
    "test-snippets": "yarn extract-snippets && yarn tsc -p codeSnippets/tsconfig.json"
  },
  "dependencies": {
    "@haved/cogsdk": "^2.32.1",
    "@haved/cogsdk-core": "^0.2.1",
    "cross-fetch": "^3.0.4",
    "lodash": "^4.17.11",
    "query-string": "^5.1.1",
    "url": "^0.11.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
