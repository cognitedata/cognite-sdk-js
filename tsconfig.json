{
    "extends": "./tsconfig.common",
    "compilerOptions": {
        "baseUrl": "packages",
        "paths": {
          "@cognite/sdk-*": ["*/src"],
          "@cognite/sdk": ["stable/src"]
        },
        "strict": true,
        "noFallthroughCasesInSwitch": true,
        "noImplicitReturns": true,
        "forceConsistentCasingInFileNames": true
    }
}