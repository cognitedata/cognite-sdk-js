{
    "extends": "./tsconfig.common",
    "compilerOptions": {
        "baseUrl": "packages",
        "paths": {
          "@cognite/sdk-*": ["*/src"],
          "@cognite/sdk": ["stable/src"]
        },
        "alwaysStrict": true,
        "noFallthroughCasesInSwitch": true,
        "noImplicitReturns": true,
        "forceConsistentCasingInFileNames": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "allowUnreachableCode": false
    }
}
